<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="FastCgiModule" scriptProcessor="D:\Python310\python.exe|D:\Python310\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
    </handlers>
    <appSettings>
      <add key="WSGI_HANDLER" value="app.app" />
      <add key="PYTHONPATH" value="D:\home\site\wwwroot" />
      <add key="WSGI_LOG" value="D:\home\LogFiles\wfastcgi.log" />
    </appSettings>
  </system.webServer>
</configuration>